{
  "extends": "eslint:recommended",
  "env": {
    "es6": true
  },
  "globals": {
    "angular": false,
    "describe": false,
    "beforeEach": false,
    "afterEach": false,
    "expect": false,
    "fail": false,
    "it": false,
    "require": false
  },
  "rules":{
    "comma-dangle": [ 0, "always-multiline" ]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}

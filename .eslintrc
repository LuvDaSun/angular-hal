{
  "extends": "eslint:recommended",
  "env": {
    "es6": true
  },
  "globals": {
    "angular": false,
    "describe": false,
    "beforeEach": false,
    "afterEach": false,
    "expect": false,
    "fail": false,
    "it": false,
    "require": false
  },
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
